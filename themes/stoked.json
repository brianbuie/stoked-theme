{
  "$schema": "vscode://schemas/color-theme",
  "name": "Stoked",
  "author": "Brian Buie",
  "semanticHighlighting": true,
  "colors": {
    "activityBar.activeBackground": "#282C34FF",
    "activityBar.activeBorder": "#F886C7FF",
