{
  "name": "theme-stoked",
  "version": "1.0.0",
  "displayName": "Stoked",
  "description": "A simple dark theme inspired by Dracula and Atom One Dark.",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Stoked",
        "uiTheme": "vs-dark",
        "path": "./themes/stoked.json"
      }
    ]
  },
  "scripts": {
    "dev": "nodemon -r esm --ignore themes ./scripts/build-vscode.js"
  },
  "devDependencies": {
    "color": "^4.2.3",
    "esm": "^3.2.25",
    "flat": "^5.0.2",
    "jsonc": "^2.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.20"
  }
}
